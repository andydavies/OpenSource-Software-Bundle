prefix=/opt/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: libarchive
Description: library that can create and read several streaming archive formats
Version: 3.1.2
Cflags: -I${includedir}
Libs: -L${libdir} -larchive
Libs.private: -llzo2 -llzma -lcharset -lbz2 -lz  -L/opt/local/lib -lxml2 -lz -lpthread -liconv -lm
